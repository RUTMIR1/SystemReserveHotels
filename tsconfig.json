{
  "compilerOptions": {
    "target": "ESNext",
    "module": "nodenext",
    "moduleResolution":"nodenext",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": [
    "src",
    "src/types/express/index.d.ts"
  ],
  "ts-node": {
  "experimentalSpecifierResolution": "node",
  "experimentalResolver": true,
  "transpileOnly": false,
  "esm": true,
  "logError": true,
}
}
